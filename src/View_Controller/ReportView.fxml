<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ReportViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="450.0" prefWidth="800.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Report View">
               <font>
                  <Font name="Roboto" size="30.0" />
               </font>
            </Text>
            <Separator focusTraversable="false" orientation="VERTICAL" prefHeight="25.0" visible="false" />
            <TextArea fx:id="reportViewTextArea" prefHeight="200.0" prefWidth="200.0" promptText="Report Text Here...">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Roboto Mono" size="12.0" />
               </font></TextArea>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
